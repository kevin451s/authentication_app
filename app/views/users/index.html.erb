<h1>Users</h1>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.name %>
      </li>
    <% end %>
  </ul>

  <% if current_user %>
    logged in as: <%= current_user.name %> | <%= link_to 'Log out', logout_path, method: :delete %>
  <% else %>
    <%= link_to 'Sign Up', new_user_path %>
    <%= link_to 'Log In', login_path %>
  <% end %>
